{{ attach_library('alparslan/tarifler') }}
{{ attach_library('alparslan/bs.buttons') }}
<div class="row">
{% for row in rows %}
  {%
    set row_classes = [
      default_row_class ? 'views-row',
    ]
  %}
  <div{{ row.attributes.addClass(row_classes).addClass("col-lg-6 col-md-6 col-sm-12 mb-5") }}>
    {{- row.content -}}
  </div>
{% endfor %}
 </div>